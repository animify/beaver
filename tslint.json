{
    "extends": [
        "tslint-react",
        "tslint-config-prettier"
    ],
    "rules": {
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-pascal-case"
        ]
    },
    "prettier": true,
    "linterOptions": {
        "exclude": [
            "node_modules/**/*.ts"
        ]
    }
}